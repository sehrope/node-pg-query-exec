{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "interface-name": false,
        "max-line-length": false,
        "quotemark": [
            true,
            "single",
            "avoid-escape",
            "avoid-template"
        ],
        "whitespace": true,
        "object-literal-sort-keys": false
    },
    "rulesDirectory": []
}
